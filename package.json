		"esbuild": "^0.18.4",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rollup": "^3.25.1",
		"rollup-plugin-dts": "^5.3.0",
		"rollup-plugin-esbuild": "^5.0.0",
		"storybook": "^7.0.21",
		"typescript": "^5.1.6",
		"vite": "^4.3.9"
	},
	"peerDependencies": {
		"styled-components": "^6.0.1"
	}
